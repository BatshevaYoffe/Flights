<Window x:Class="PL.DatesAndFlightsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="DatesAndFlightsWindow" Height="450" Width="800">
    <Grid>
        <Button Command="{Binding SelectDates}" CommandParameter="{Binding ConverterParameter= }" Content="Select Dates" HorizontalAlignment="Left" Height="50" Margin="344,10,0,0" VerticalAlignment="Top" Width="172"/>
        <Calendar HorizontalAlignment="Left" Margin="485,92,0,0" VerticalAlignment="Top" SelectedDatesChanged="selectedDates" x:Name="calender"/>
        
    </Grid>
</Window>
